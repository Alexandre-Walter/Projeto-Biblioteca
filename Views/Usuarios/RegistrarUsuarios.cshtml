@model Usuario

<h1>Registre um Usuário</h1>

<form action="RegistrarUsuario" method="post">
    <input type="text" name="Nome" id="userNome" placehold="Nome">
    <input type="text" name="login" id="userLogin" placehold="Login">
    <input type="text" name="senha" id="userSenha" placehold="Senha">

    <select name="tipo" id="userTipo">
        <option value="@Usuario.ADMIN">Adminitrador</option>
        <option value="@Usuario.PADRAO">Padrão</option>
    </select>

    <input type="submit" value="REGISTRAR">
</form>